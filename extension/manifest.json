{
  "name": "MOState Course Browser",
  "description": "Help browse different courses by title and organize your next schedule.",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*.missouristate.edu/*", "https://*.missouristate.edu/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
      "default_title": "Set this page's color.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
